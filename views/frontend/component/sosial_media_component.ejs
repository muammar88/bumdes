<h4 class="title-footer mx-auto">Sosial Media</h4>
<ul class="list-group">
     <% for ( let index = 0; index < Object.keys(sosial_media).length; index ++ ) { %>
          <li class="list-group-item d-flex  border-0 px-0">
               <i class="<%= sosial_media[index].icon %> pt-1 mr-2"></i>
               <%= sosial_media[index].name %> 
          </li>
     <% } %>
</ul>
